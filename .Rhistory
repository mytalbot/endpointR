library(endpointR)
library(RELSA)
# Load Laura Act Data -----------------------------------------------------
raw       <- relsa_load("C:/MHH Bleich/Papers/PostOPPaper/Data/svenja_dss_mod.txt" )
raw       <- raw[, c(1, 2, 4, 5)]
# Chose animal ------------------------------------------------------------
a         <- 31
dat       <- raw[raw$id==unique(raw$id)[a],  c(1,3,4)]
td        <- reshape(dat, idvar = "id", timevar = "day", direction = "wide")
